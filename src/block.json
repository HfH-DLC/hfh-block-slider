{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "hfh/slider",
    "version": "0.1.0",
    "title": "Hfh Slider",
    "category": "widgets",
    "icon": "images-alt2",
    "description": "Page Slider.",
    "supports": {
        "html": false,
        "align": ["wide"]
    },
    "textdomain": "hfh-slider",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "attributes": {
        "categoryId": {
            "type": "integer"
        },
        "numberOfSlides": {
            "type": "integer",
            "default": 6
        },
        "overlap": {
            "type": "boolean",
            "default": true
        }
    }
}